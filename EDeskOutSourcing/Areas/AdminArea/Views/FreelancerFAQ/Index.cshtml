@model IEnumerable<FreelancerFAQ>
<h1>Freelancer FAQ List</h1>
<a asp-action="Create" class="btn btn-success">Create New</a>
<div>
    <table class="table table-bordered">
        <thead>
            <tr>
                <td>Freelancer FAQ ID</td>
                <td>FAQ </td>
                <td>Answer</td>
                <td>Added Date</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var temp in Model)
            {
                <tr>
                    <td>@temp.FreelancerFAQID</td>
                    <td>@temp.FAQ</td>
                    <td>@temp.Answer</td>
                    <td>@temp.AddedDate</td>
                    <td>
                        <a asp-action="Edit" asp-controller="FreelancerFAQ" class="btn btn-success" asp-route-id="@temp.FreelancerFAQID">Edit</a> 
                        <a asp-action="Delete" asp-controller="FreelancerFAQ" class="btn btn-danger" asp-route-id="@temp.FreelancerFAQID">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>