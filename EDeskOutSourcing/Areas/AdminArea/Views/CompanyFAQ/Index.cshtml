@model IEnumerable<CompanyFAQ>
<h1>Company FAQ List</h1>
<a asp-action="Create" class="btn btn-success">Create New</a>
<div>
    <table class="table table-bordered">
        <thead>
            <tr>
                <td>Company FAQ ID</td>
                <td>FAQ </td>
                <td>Answer</td>
                <td>Added Date</td>
            </tr>
        </thead>
        <tbody>
            @foreach(var temp in Model)
            {
                <tr>
                    <td>@temp.CompanyFAQID</td>
                    <td>@temp.FAQ</td>
                    <td>@temp.Answer</td>
                    <td>@temp.AddedDate</td>
                    <td>
                        <a asp-action="Edit" asp-controller="CompanyFAQ" class="btn btn-success" asp-route-id="@temp.CompanyFAQID">Edit</a>
                        <a asp-action="Delete" asp-controller="CompanyFAQ" class="btn btn-danger" asp-route-id="@temp.CompanyFAQID">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>