@model IEnumerable<Technologies>
<h1>Technologies List</h1>
<a asp-action="Create" class="btn btn-success">Create New</a>
<div>
    <table class="table table-bordered">
        <thead>
            <tr>
                <td>Technology ID</td>
                <td>Technology Name </td>
            </tr>
        </thead>
        <tbody>
            @foreach (var temp in Model)
            {
                <tr>
                    <td>@temp.TechnologyID</td>
                    <td>@temp.TechnologyName</td>
                    <td>
                        <a asp-action="Edit" asp-controller="Technologies" class="btn btn-success" asp-route-id="@temp.TechnologyID">Edit</a>
                        <a asp-action="Delete" asp-controller="Technologies"class="btn btn-danger"  asp-route-id="@temp.TechnologyID">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>