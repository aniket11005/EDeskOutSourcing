@model IEnumerable<FreelancerExperience>
@{
    Layout = "~/Areas/FreelancerArea/Views/Shared/_Layout.cshtml";
}
<h1>Index Details</h1>
<a asp-action="Create" asp-controller="FreelancerExperience" asp-area="FreelancerArea" class="btn btn-success">Create</a>
<table class="table table-bordered">
    <thead>
        <tr>
            <td>Experience In Months</td>
            <td>Role Name </td>
        </tr>
    </thead>
    <tbody>
        @foreach (var temp in Model)
        {
            <tr>
                <td>@temp.ExperienceInMonths</td>
                <td>
                   @temp.RoleName
                </td>
                <td>
                    <a asp-action="Edit" asp-controller="FreelancerExperience" asp-area="FreelancerArea" asp-route-id="@temp.FreelancerExperienceID" class="btn btn-success">Edit</a>
                    <a asp-action="Delete" asp-controller="FreelancerExperience" asp-area="FreelancerArea" asp-route-id="@temp.FreelancerExperienceID" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>