      <!-- Navbar Right Menu-->
      <ul class="app-nav">
        <li class="app-search">
          <input class="app-search__input" type="search" placeholder="Search">
          <button class="app-search__button"><i class="fa fa-search"></i></button>
        </li>
       
        <!-- User Menu-->
        <li class="dropdown"><a class="app-nav__item" href="#" data-toggle="dropdown" aria-label="Open Profile Menu"><i class="fa fa-user fa-lg"></i></a>
          <ul class="dropdown-menu settings-menu dropdown-menu-right">
            <li><a class="dropdown-item" asp-action="EditProfile" asp-controller="FreelancerHome" asp-area="FreelancerArea"><i class="fa fa-user fa-lg"></i>Edit Profile</a></li>
            <li><a class="dropdown-item" asp-action="Index" asp-controller="Home" asp-area=""><i class="fa fa-sign-out fa-lg"></i> Logout</a></li>
            <li><a class="dropdown-item" asp-controller="FreelancerHome" asp-action="ChangePassword" asp-area="FreelancerArea"><i class="fa fa-cog fa-lg"></i> Change Password</a></li>

          </ul>
        </li>
      </ul>
