@model IEnumerable<ProjectAssignedVM>
@{
    Layout = "~/Areas/FreelancerArea/Views/Shared/_Layout.cshtml";
}
<h1>Index Details</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Project Name</th>
            <th>Assign Date</th>
            <th>Remark </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var temp in Model)
        {
            <tr>
                <td>@temp.ProjectName</td>
                <td>@temp.AssignDate</td>
                <td>
                    @temp.Remark
                </td>
               @* <td>
                    <a asp-action="Edit" asp-controller="FreelancerExperience" asp-area="FreelancerArea" asp-route-id="@temp.FreelancerExperienceID" class="btn btn-success">Edit</a>
                    <a asp-action="Delete" asp-controller="FreelancerExperience" asp-area="FreelancerArea" asp-route-id="@temp.FreelancerExperienceID" class="btn btn-success">Delete</a>
                </td>*@
            </tr>
        }
    </tbody>
</table>